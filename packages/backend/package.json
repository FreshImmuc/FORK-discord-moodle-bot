{
  "name": "@fmdb/backend",
  "version": "1.0.0",
  "description": "API&Bot backend of the FMDB-Project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rd dist /s /q || rm -r dist/",
    "lint": "npm run prebuild",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "prestart": "npm run build",
    "start": "concurrently -c \"yellow.bold,orange.bold\" -n \"WATCH,RUN\" \"npm run build:watch\" \"nodemon --ignore dist/docs/ .\"",
    "test": "echo \"Add tests please\""
  },
  "dependencies": {
    "express": "^4.17.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.5",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  }
}
